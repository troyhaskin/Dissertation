@article{marcel_experimental_2009,
	title = {Experimental and numerical investigations on flashing-induced instabilities in a single channel},
	volume = {33},
	issn = {0894-1777},
	url = {http://www.sciencedirect.com/science/article/pii/S0894177709001204},
	doi = {10.1016/j.expthermflusci.2009.08.001},
	abstract = {During the start-up phase, natural circulation BWRs (NC-BWRs) need to be operated at low pressure conditions. Such conditions favor flashing-induced instabilities due to the large hydrostatic pressure drop induced by the tall chimney. Moreover, in novel NC-BWR designs the steam separation is performed in the steam separators which create large pressure drops at the chimney outlet, which effect on stability has not been investigated yet. In this work, flashing-induced oscillations occurring in a tall, bottom heated channel are numerically investigated by using a simple linear model with three regions and an accurate implementation for estimating the water properties. The model is used to investigate flashing-induced instabilities in a channel for different values of the core inlet friction value. The results are compared with experiments obtained by using the CIRCUS facility at the same conditions, showing a good agreement. In addition, the experiments on flashing-induced instabilities are presented in a novel manner allowing visualizing new details of the phenomenon numerical stability investigations on the effect of the friction distribution are also done. It is found that by increasing the total restriction in the channel the system is destabilized. In addition, the chimney outlet restriction has a stronger destabilizing effect than the core inlet restriction. A stable two-phase region is observed prior to the instabilities in the experiments and the numerical simulations which may help to pressurize the vessel of NC-BWRs and thus reducing the effects of flashing instabilities during start-up.},
	number = {8},
	urldate = {2012-12-03},
	journal = {Experimental Thermal and Fluid Science},
	author = {Marcel, Christian P. and Rohde, M. and Van Der Hagen, T.H.J.J.},
	month = nov,
	year = {2009},
	keywords = {Experiments, Flashing-induced instabilities, Natural circulation boiling water reactors, Stability},
	pages = {1197--1208},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Troy Haskin\\Literature\\storage\\7II7T96H\\Marcel et al. - 2009 - Experimental and numerical investigations on flash.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Troy Haskin\\Literature\\storage\\REZ4BZ57\\Marcel et al. - 2009 - Experimental and numerical investigations on flash.html:text/html}
}

