	keywords = {Algorithms, Appl.Mathematics/Computational Methods of Engineering, Computational Mathematics and Numerical Analysis, High order accuracy, Mathematical and Computational Physics, Multistep methods, Runge–Kutta methods, Spectral deferred correction methods, Strong stability preserving, Time discretization},
	pages = {251--289},
	file = {Full Text PDF:C\:\\Users\\Troy Haskin\\Literature\\storage\\TAP23J6S\\Gottlieb et al. - 2009 - High Order Strong Stability Preserving Time Discre.pdf:application/pdf;Snapshot:C\:\\Users\\Troy Haskin\\Literature\\storage\\3VKHX8T9\\Gottlieb et al. - 2009 - High Order Strong Stability Preserving Time Discre.html:text/html}
