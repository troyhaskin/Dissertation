  pages = {165--192}
}

@article{misale_experiments_2007-1,
  title = {Experiments in a Single-Phase Natural Circulation Mini-Loop},
  volume = {31},
  issn = {0894-1777},
  doi = {10.1016/j.expthermflusci.2006.11.004},
