@article{walker_simpler_1994,
  title = {A Simpler {{GMRES}}},
  volume = {1},
  issn = {1099-1506},
  doi = {10.1002/nla.1680010605},
  abstract = {The generalized minimal residual (GMRES) method is widely used for solving very large, nonsymmetric linear systems, particularly those that arise through discretization of continuous mathematical models in science and engineering. By shifting the Arnoldi process to begin with Ar0 instead of r0, we obtain simpler Gram\textendash{}Schmidt and Householder implementations of the GMRES method that do not require upper Hessenberg factorization. The Gram\textendash{}Schmidt implementation also maintains the residual vector at each iteration, which allows cheaper restarts of GMRES(m) and may otherwise be useful.},
