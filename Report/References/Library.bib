  author = {Giaquinta, Mariano and Hildebrandt, Stefan},
  year = {1996},
  keywords = {Mathematics / Calculus,Mathematics / Functional Analysis,Mathematics / General,Mathematics / Geometry / Differential,Mathematics / Linear \& Nonlinear Programming,Science / Physics / Mathematical \& Computational}
}

