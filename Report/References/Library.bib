@article{winning_explicit_2012,
  title = {Explicit {{Friction Factor Accuracy}} and {{Computational Efficiency}} for {{Turbulent Flow}} in {{Pipes}}},
  volume = {90},
  issn = {1386-6184, 1573-1987},
  doi = {10.1007/s10494-012-9419-7},
  abstract = {The implicit Colebrook\textendash{}White equation is the accepted method for accurately estimating the friction factor for turbulent flow in pipes. This study reviews 28 explicit equations for approximating the friction factor to integrate both the accuracy to the implicit Colebrook\textendash{}White equation and the relative computational efficiency of the explicit equations. A range of 901 Reynolds numbers were selected for the review between Re $\geq$ 4 \texttimes{}103 and $\leq$ 4 \texttimes{} 108 and 20 relative pipe roughness values were selected between $\epsilon$/$\epsilon$DD$\geq$10-6$\leq$10-1$\epsilon$/$\epsilon$DD$\geq$10-6$\leq$10-1$\backslash$varepsilon $\backslash$mathord\{$\backslash$left/ \{$\backslash$vphantom \{$\backslash$varepsilon D\}\} $\backslash$right.\}D$\backslash$ge 10\^\{-6\}$\backslash$le 10\^\{-1\}, thus producing a matrix of 18,020 points for each explicit equation, covering all the values to be encountered in pipeline hydraulic analysis for turbulent flow. The accuracy of the estimation of the friction factor using the explicit equations to the value obtained using the implicit Colebrook\textendash{}White equation were calculated and reported as absolute, relative percentage and mean square errors. To determine the relative computational efficiency, 300 million friction factor calculations were performed using randomly generated values for the Reynolds number and the relative pipe roughness values between the limits specified for each of the explicit equations and compared to the time taken by the Colebrook\textendash{}White equation. Finally, 2D and 3D contour models were generated showing both the range and magnitude of the relative percentage accuracy across the complete range of 18,020 points for each explicit equation.},
  language = {en},
  timestamp = {2016-08-22T23:45:14Z},
  number = {1},
  urldate = {2016-08-22},
  journal = {Flow, Turbulence and Combustion},
  author = {Winning, Herbert Keith and Coole, Tim},
  month = oct,
  year = {2012},
  pages = {1--27},
  file = {Full Text PDF:C\:\\Users\\Troy Haskin 2\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\e2mwu9hy.default\\zotero\\storage\\6RAWWM78\\Winning and Coole - 2012 - Explicit Friction Factor Accuracy and Computationa.pdf:application/pdf;Snapshot:C\:\\Users\\Troy Haskin 2\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\e2mwu9hy.default\\zotero\\storage\\WKQBR49D\\s10494-012-9419-7.html:text/html}
}

