  author = {Guanghui, Su and Dounan, Jia and Fukuda, Kenji and Yujun, Guo},
  month = jun,
  year = {2002},
  pages = {187--198}
}

@article{gottlieb_high_2009-2,
  title = {High {{Order Strong Stability Preserving Time Discretizations}}},
  volume = {38},
  issn = {0885-7474, 1573-7691},
  doi = {10.1007/s10915-008-9239-z},
  abstract = {Strong stability preserving (SSP) high order time discretizations were developed to ensure nonlinear stability properties necessary in the numerical solution of hyperbolic partial differential equations with discontinuous solutions. SSP methods preserve the strong stability properties\textemdash{}in any norm, seminorm or convex functional\textemdash{}of the spatial discretization coupled with first order Euler time stepping. This paper describes the development of SSP methods and the connections between the timestep restrictions for strong stability preservation and contractivity. Numerical examples demonstrate that common linearly stable but not strong stability preserving time discretizations may lead to violation of important boundedness properties, whereas SSP methods guarantee the desired properties provided only that these properties are satisfied with forward Euler timestepping. We review optimal explicit and implicit SSP Runge\textendash{}Kutta and multistep methods, for linear and nonlinear problems. We also discuss the SSP properties of spectral deferred correction methods.},
  language = {en},
  timestamp = {2016-08-19T03:32:04Z},
  number = {3},
  urldate = {2012-12-03},
  journal = {Journal of Scientific Computing},
  author = {Gottlieb, Sigal and Ketcheson, David I. and Shu, Chi-Wang},
  month = mar,
  year = {2009},
  keywords = {Algorithms,Appl.Mathematics/Computational Methods of Engineering,Computational Mathematics and Numerical Analysis,High order accuracy,Mathematical and Computational Physics,Multistep methods,Runge–Kutta methods,Spectral deferred correction methods,Strong stability preserving,Time discretization},
  pages = {251--289}
}

@article{vijayan_steady_2007-1,
  title = {Steady State and Stability Characteristics of Single-Phase Natural Circulation in a Rectangular Loop with Different Heater and Cooler Orientations},
  volume = {31},
  issn = {0894-1777},
  doi = {10.1016/j.expthermflusci.2006.10.003},
  abstract = {Effect of the heater and cooler orientations on the single-phase natural circulation was studied in a rectangular loop. From steady state considerations, the maximum flow for a specified operating condition is achievable for the orientation with both the heater and the cooler horizontal. However, this orientation is found to be least stable and the orientation with both heater and cooler vertical is found to be most stable. Three oscillatory modes were observed in the experiments, two of which are observable only for certain heat addition paths. A hysteresis (conditionally stable) regime wherein stable or unstable flow can prevail depending on the operating procedure is also observed. Generalized steady state equation valid for all the orientations is possible whereas generalized stability equation is possible for a given orientation and flow direction. Steady state flow rate can be predicted with reasonable accuracy using the generalized equation. The stability map predicted by the generalized equation is found to be highly conservative in that its unstable zone is much larger than that observed in the experiments. Both the linear and the nonlinear stability analyses gave the same stability map for identical initial conditions. With nonlinear analysis, it was possible to reproduce the observed unstable oscillatory modes albeit at significantly different power levels. Also, the predicted limit cycles were significantly different in shape which is attributed to the multidimensional nature of the flow especially during the low flow part of the oscillation cycle.},
  timestamp = {2016-08-19T03:32:04Z},
  number = {8},
  urldate = {2012-12-03},
  journal = {Experimental Thermal and Fluid Science},
  author = {Vijayan, P.K. and Sharma, M. and Saha, D.},
  month = aug,
  year = {2007},
  keywords = {Conditionally stable regime,Hysteresis,instability,Natural circulation},
  pages = {925--945}
}

@article{almenas_unstable_1991-1,
  title = {An Unstable, Condensation Controlled Two-Phase Natural Circulation Mode},
  volume = {131},
  issn = {0029-5493},
  doi = {10.1016/0029-5493(91)90282-M},
  abstract = {This study analyzes a transient behavior observed during natural-circulation flow which is not easily placed within the reviewed context. Most of the documented instabilities are either of the ``surge'' type which are driven by inertial effects or changes in flow regimes (this includes quench-dryout instabilities and condensation driven ``chugging''), or they depend on loop wide feedback effects and consequently have a frequency proportional to the fluid transit time (density wave). Although the observed dynamic behavior incorporates some features of both categories, it cannot be adequately described by these mechanisms. It is shown that the system response consists of two sequentially alternating, relatively stable flow phases, which have a source-sink imbalance of opposite sign. These flow phases can persist for lengthy periods of time (long in comparison to the fluid transit time). This is of importance because it implies that in analyzing the behavior of the system, time averaging methods cannot be used.},
  timestamp = {2016-08-19T03:32:04Z},
  number = {2},
  urldate = {2012-12-03},
  journal = {Nuclear Engineering and Design},
  author = {Almenas, K. and Wu, C.C. and Ragulskis, L. and {diMarzo}, M.},
