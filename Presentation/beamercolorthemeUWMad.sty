\mode<presentation>

\definecolor{UWMadDarkRed}{rgb}{0.59766,0.00000,0.00000}
\definecolor{UWMadRed}    {rgb}{0.71484,0.00000,0.00000}
\definecolor{UWMadGold}   {rgb}{0.79688,0.59766,0.00000}
\definecolor{UWMadGray}   {rgb}{0.35156,0.35156,0.35156}



\setbeamercolor{section in toc}{fg=UWMadGray,bg=white}
\setbeamercolor{alerted text}{fg=UWMadDarkRed!80!gray}
\setbeamercolor*{palette primary}{fg=white,bg=UWMadRed}
\setbeamercolor*{palette secondary}{fg=UWMadDarkRed,bg=white}
\setbeamercolor*{palette tertiary}{bg=UWMadDarkRed,fg=white}
\setbeamercolor*{palette quaternary}{fg=UWMadDarkRed,bg=UWMadGray}

\setbeamercolor*{item projected}{fg=white,bg=UWMadRed}
\setbeamercolor*{itemize       item}{fg=UWMadRed,bg=UWMadRed}
\setbeamercolor*{itemize    subitem}{fg=UWMadRed,bg=UWMadRed}
\setbeamercolor*{itemize subsubitem}{fg=UWMadRed,bg=UWMadRed}
\setbeamercolor*{description item}{fg=UWMadRed,bg=white}

\setbeamercolor{section in head/foot}{fg=white,bg=UWMadDarkRed}

\setbeamercolor{titlelike}{parent=palette primary}
\setbeamercolor{caption name}{parent=palette secondary}
\setbeamercolor{frametitle}{fg=white,bg=UWMadRed}
\setbeamercolor{frametitle right}{bg=gray!60!white}

\setbeamercolor*{fine separation line}{bg=UWMadGold}

\mode
<all>

